<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/more_nav"
    app:startDestination="@id/morePageFragment">

    <fragment
        android:id="@+id/profileFragment"
        android:name="com.bekircaglar.budgetcontrol.fragments.details.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" >
        <action
            android:id="@+id/action_profileFragment_to_morePageFragment"
            app:destination="@id/morePageFragment" />
        <action
            android:id="@+id/action_profileFragment_to_helpFragment"
            app:destination="@id/helpFragment" />
    </fragment>
    <fragment
        android:id="@+id/helpFragment"
        android:name="com.bekircaglar.budgetcontrol.fragments.details.HelpFragment"
        android:label="fragment_help"
        tools:layout="@layout/fragment_help" >
        <action
            android:id="@+id/action_helpFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_helpFragment_to_morePageFragment"
            app:destination="@id/morePageFragment" />
    </fragment>
    <fragment
        android:id="@+id/morePageFragment"
        android:name="com.bekircaglar.budgetcontrol.fragments.details.MorePageFragment"
        android:label="fragment_more_page"
        tools:layout="@layout/fragment_more_page" >
        <action
            android:id="@+id/action_morePageFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_morePageFragment_to_helpFragment"
            app:destination="@id/helpFragment" />
        <action
            android:id="@+id/action_morePageFragment_to_accountsFragment"
            app:destination="@id/accountsFragment" />
    </fragment>
    <fragment
        android:id="@+id/accountsFragment"
        android:name="com.bekircaglar.budgetcontrol.fragments.details.AccountsFragment"
        android:label="fragment_accounts"
        tools:layout="@layout/fragment_accounts" >
        <action
            android:id="@+id/action_accountsFragment_to_morePageFragment"
            app:destination="@id/morePageFragment" />
    </fragment><![CDATA[

    />

]]>
